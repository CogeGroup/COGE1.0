	<variable name="MARGINE_MEDIO" class="java.lang.Number">
		<variableExpression><![CDATA[($V{RICAVO_TOTALE} - $V{COSTO_TOTALE}) / $V{RICAVO_TOTALE}]]></variableExpression>
	</variable>
