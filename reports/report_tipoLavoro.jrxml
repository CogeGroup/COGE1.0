	</parameter>
	<parameter name="DATA_FINE" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select count(*) as numRisorse,
t.idTipoRapportoLavoro as idTipoRapportoLavoro, t.rappLavoro_cod as codice, t.rappLavoro_desc as descrizione,
sum(t.ore_lavorate) as oreLavorate,
sum(t.costo_totale) as costoTot,
sum(t.ricavo_totale) as ricavoTotale,
margine(sum(t.costo_totale), sum(t.ricavo_totale)) as margineTotale
from(
SELECT distinct r.idRisorsa as idRisorsa, r.matricola as matricola,
trl.idTipoRapportoLavoro as idTipoRapportoLavoro, trl.codice as rappLavoro_cod, trl.descrizione as rappLavoro_desc,
sum(if(com.calcoloRicavi=true,ra.oreLavorate,0)) as ore_lavorate,
if(c.importoMensile is null, sum(costo(c.importoGiornaliero,ra.oreLavorate)), c.importoMensile) as costo_totale,
sum(ricavo(t.importoGiornaliero,ra.oreLavorate)) as ricavo_totale,
if(c.importoMensile is null, sum(margine(costo(c.importoGiornaliero,ra.oreLavorate), ricavo(t.importoGiornaliero,ra.oreLavorate))), sum(margine(c.importoMensile, ricavo(t.importoGiornaliero,ra.oreLavorate)))) as margine_totale,
t.importoGiornaliero as tariffa_importo
FROM `Risorsa` r, `Costo` c, `Tariffa` t, `Commessa` com, `RendicontoAttivita` ra,`RapportoLavoro` rl,`TipoRapportoLavoro` trl
WHERE r.idRisorsa = c.risorsa_idRisorsa and c.dataInizio <= $P{DATA_INIZIO}
and (c.dataFine is null or c.dataFine >= $P{DATA_FINE})
and r.idRisorsa = t.risorsa_idRisorsa and t.commessa_idCommessa = com.idCommessa
and t.dataInizio <= $P{DATA_INIZIO}
and (t.dataFine is null or t.dataFine >= $P{DATA_FINE})
and r.idRisorsa = rl.risorsa_idRisorsa and rl.dataInizio <= $P{DATA_INIZIO}
and (rl.dataFine is null or rl.dataFine >= $P{DATA_FINE})
and r.idRisorsa = ra.risorsa_idRisorsa and ra.commessa_idCommessa = com.idCommessa
and rl.dataInizio <= $P{DATA_INIZIO}
and (rl.dataFine is null or rl.dataFine >= $P{DATA_FINE})
and rl.tipoRapportoLavoro_idTipoRapportoLavoro = trl.idTipoRapportoLavoro
and r.idRisorsa = rl.risorsa_idRisorsa
and ra.mese= $P{MESE}
and ra.anno= $P{ANNO}
group by r.idRisorsa, r.matricola
) as t
group by t.idTipoRapportoLavoro]]>
	</queryString>
