	</parameter>
	<parameter name="ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT concat(r.cognome,' ',r.nome) AS risorsa,
IF(c.importoMensile is null, costo(c.importoGiornaliero,ra.oreLavorate), c.importoMensile) AS costo_totale,
SUM(ricavo(t.importoGiornaliero,ra.oreLavorate)) AS ricavo_totale
FROM `Risorsa` r, `Costo` c, `Tariffa` t, `Commessa` com, `RendicontoAttivita` ra
WHERE com.idCommessa = $P{ID}
AND ra.mese= $P{MESE}
AND ra.anno= $P{ANNO}
AND ra.commessa_idCommessa = com.idCommessa
AND ra.risorsa_idRisorsa = r.idRisorsa
AND ra.costo_idCosto = c.idCosto
AND t.risorsa_idRisorsa = r.idRisorsa
AND t.commessa_idCommessa = ra.commessa_idCommessa
AND ((t.dataInizio <= $P{DATA_INIZIO} AND (t.dataFine is null OR t.dataFine >= $P{DATA_FINE})) OR (t.dataInizio <= $P{DATA_INIZIO} AND t.dataFine >= $P{DATA_INIZIO} AND t.dataFine <= $P{DATA_FINE}) OR (t.dataInizio >= $P{DATA_INIZIO} AND t.dataInizio <= $P{DATA_FINE} AND (t.dataFine is null OR t.dataFine >= $P{DATA_FINE})) OR (t.dataInizio > $P{DATA_INIZIO} AND t.dataFine < $P{DATA_FINE}))
GROUP BY idRisorsa]]>
	</queryString>
