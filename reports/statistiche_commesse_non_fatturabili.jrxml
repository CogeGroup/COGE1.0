	<parameter name="ANNO" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select com.codice,com.descrizione,
sum(ra.oreLavorate) as ore_totali,
sum(if(ra.mese <= 6,ra.oreLavorate,0)) as ore_totali_primo_semestre,
sum(if(ra.mese > 6,ra.oreLavorate,0)) as ore_totali_secondo_semestre,
sum(if(ra.mese = 1, ra.oreLavorate,0)) as totale_gennaio,
sum(if(ra.mese = 2, ra.oreLavorate,0)) as totale_febbraio,
sum(if(ra.mese = 3, ra.oreLavorate,0)) as totale_marzo,
sum(if(ra.mese = 4, ra.oreLavorate,0)) as totale_aprile,
sum(if(ra.mese = 5, ra.oreLavorate,0)) as totale_maggio,
sum(if(ra.mese = 6, ra.oreLavorate,0)) as totale_giugno,
sum(if(ra.mese = 7, ra.oreLavorate,0)) as totale_luglio,
sum(if(ra.mese = 8, ra.oreLavorate,0)) as totale_agosto,
sum(if(ra.mese = 9, ra.oreLavorate,0)) as totale_settembre,
sum(if(ra.mese = 10, ra.oreLavorate,0)) as totale_ottobre,
sum(if(ra.mese = 11, ra.oreLavorate,0)) as totale_novembre,
sum(if(ra.mese = 12, ra.oreLavorate,0)) as totale_dicembre
  FROM RendicontoAttivita ra
  inner join Commessa com on ra.commessa_idCommessa=com.idCommessa
  where com.DTYPE = 'Commessa' and com.fatturabile=false and ra.anno=$P{ANNO}
  group by com.codice,com.descrizione]]>
	</queryString>
